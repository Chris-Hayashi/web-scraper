<div id="sports">
  <h1>This tag is a test</h1>
  {{!-- <img src="/assets/images/scraperLogo.png" id="scraperLogo" class="title title-logo"> --}}


    {{#each sports}}

      {{#if saved}}
      <center>
        <div class="eachCard">
          <div class="card w-75">
          <div class="card-body">
            {{!-- <h5 class="card-title">{{this.title}}</h5> --}}
            <p class="card-text">{{this.title}}</p>
            <button><a href={{this.link}} class="btn btn-primary">Save Article</a></button>
          </div>
        </div>
      </center>
      <br><br>
      {{/if}}

    {{/each}}

</div>